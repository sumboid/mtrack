{
  "tasks": {
    "dev": "deno run --allow-all npm:vite",
    "build": "deno run --allow-all npm:vite build",
    "lint": "deno run --allow-all npm:eslint .",
    "preview": "deno run --allow-all npm:vite preview"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "imports": {
    "@emotion/react": "npm:@emotion/react@^11.14.0",
    "@emotion/styled": "npm:@emotion/styled@^11.14.1",
    "@eslint/js": "npm:@eslint/js@^9.36.0",
    "@mui/icons-material": "npm:@mui/icons-material@^7.3.4",
    "@mui/material": "npm:@mui/material@^7.3.4",
    "@mui/x-date-pickers": "npm:@mui/x-date-pickers@^8.12.0",
    "@types/react": "npm:@types/react@^19.2.0",
    "@types/react-dom": "npm:@types/react-dom@^19.2.0",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^5.0.4",
    "@xstate/react": "npm:@xstate/react@^6.0.0",
    "dayjs": "npm:dayjs@^1.11.13",
    "dexie": "npm:dexie@^4.2.1",
    "eslint": "npm:eslint@^9.36.0",
    "eslint-plugin-react-hooks": "npm:eslint-plugin-react-hooks@^6.1.0",
    "eslint-plugin-react-refresh": "npm:eslint-plugin-react-refresh@^0.4.23",
    "globals": "npm:globals@^16.4.0",
    "i18next": "npm:i18next@^25.5.3",
    "i18next-browser-languagedetector": "npm:i18next-browser-languagedetector@^8.2.0",
    "nanoid": "npm:nanoid@^5.1.6",
    "react": "npm:react@^19.2.0",
    "react-dom": "npm:react-dom@^19.2.0",
    "react-i18next": "npm:react-i18next@^16.0.0",
    "react-router-dom": "npm:react-router-dom@^7.9.3",
    "rolldown-vite": "npm:rolldown-vite@^7.1.15",
    "typescript": "npm:typescript@^5.9.3",
    "typescript-eslint": "npm:typescript-eslint@^8.45.0",
    "xstate": "npm:xstate@^5.22.1",
    "zod": "npm:zod@^4.1.11"
  }
}
